{
    "compilerOptions": {
        "declaration": false,
        "noResolve": false,
        "skipLibCheck": true,
        "jsx": "react",
        "module": "commonjs",
        "target": "es5",
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "outDir": "./rawjs"
    },
    "filesGlob": [
        "ts/**/*{ts,tsx}"
    ],
    "exclude": [
        "node_modules"
    ]
}
